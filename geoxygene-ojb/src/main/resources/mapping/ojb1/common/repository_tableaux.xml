  <field-descriptor name="id" column="COGITID" jdbc-type="INTEGER" primarykey="true" autoincrement="true" />
  <field-descriptor name="geom" column="GEOM" jdbc-type="STRUCT" conversion="fr.ign.cogit.geoxygene.datatools.ojb.GeomGeOxygene2Dbms"/>
  <field-descriptor name="doubles" column="DOUBLES" jdbc-type="ARRAY" conversion="fr.ign.cogit.geoxygene.datatools.oracle.ArrayGeOxygene2Oracle"/> 
  <field-descriptor name="ints" column="INTS" jdbc-type="ARRAY" conversion="fr.ign.cogit.geoxygene.datatools.oracle.ArrayGeOxygene2Oracle"/> 
  <field-descriptor name="booleans" column="BOOLEANS" jdbc-type="ARRAY" conversion="fr.ign.cogit.geoxygene.datatools.oracle.ArrayGeOxygene2Oracle"/>
  <field-descriptor name="strings" column="STRINGS" jdbc-type="ARRAY" conversion="fr.ign.cogit.geoxygene.datatools.oracle.ArrayGeOxygene2Oracle"/>  
</class-descriptor>   
